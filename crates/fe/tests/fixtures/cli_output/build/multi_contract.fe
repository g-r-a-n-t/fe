use core::abi::{Abi, AbiEncoder, AbiSize, Encode}
use std::abi::Sol

msg FooMsg {
    #[selector = 0x12345678]
    Ping -> u256,
}

pub contract Foo {
    recv FooMsg {
        Ping -> u256 {
            return 1
        }
    }
}

msg BarMsg {
    #[selector = 0x87654321]
    Pong -> u256,
}

pub contract Bar {
    recv BarMsg {
        Pong -> u256 {
            return 2
        }
    }
}

