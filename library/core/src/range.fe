use ingot::seq::Seq

/// A range from start to end (exclusive): `start..end`
pub struct Range {
    pub start: usize,
    pub end: usize,
}

impl Range {
    pub fn new(start: usize, end: usize) -> Self {
        Range { start, end }
    }
}

impl Seq<usize> for Range {
    fn len(self) -> usize {
        self.end - self.start
    }

    fn get(self, i: usize) -> usize {
        self.start + i
    }
}
